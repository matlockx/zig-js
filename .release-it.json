{
  "non-interactive": true,
  "requireCleanWorkingDir": false,
  "increment": "patch",
  "src": {
    "tagName": "v%s",
    "afterReleaseCommand": "bash -c 'if [[ ${version} != *beta* ]] ; then ./publish-s3.sh ; fi'"
  },
  "buildCommand": "./build.sh",
  "github": {
    "release": false
  },
  "npm": {
    "publish": true,
    "publishPath": "dist"
  }
}